CREATE TABLE books (id INTEGER PRIMARY KEY,name TEXT, rating INTEGER);

INSERT INTO books VALUES (1, "The Paris Apartment", 1);
INSERT INTO books VALUES (2, "Option B", 2);
INSERT INTO books VALUES (3, "Love In The Time of Serial Killers", 3);
INSERT INTO books VALUES (4, "Spare", 4);

--display the database ordered by rating 
SELECT * FROM books
ORDER BY price asc; 
